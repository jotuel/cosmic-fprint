<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
  <id>fi.joonastuomi.CosmicFprint</id>
  <metadata_license>CC0-1.0</metadata_license>
  <project_license>MPL-2.0</project_license>
  <name>Fprint Extension</name>
  <summary>GUI for fprintd fingerprint enrolling</summary>
  <description>
    <p>
      Fprint Extension is an application for enrolling fingerprints using the fprintd daemon. It allows you to register and delete fingerprints for authentication on your Linux system.
    </p>
  </description>
  <icon type="remote" width="64" height="64" scale="1">
    https://github.com/jotuel/cosmic-ext-fprint/raw/main/resources/icons/hicolor/scalable/apps/icon.svg
  </icon>
  <url type="vcs-browser">https://github.com/jotuel/cosmic-ext-fprint</url>
  <launchable type="desktop-id">fi.joonastuomi.CosmicFprint.desktop</launchable>
  <provides>
    <id>fi.joonastuomi.CosmicFprint</id>
    <binaries>
      <binary>cosmic-ext-fprint</binary>
    </binaries>
  </provides>
  <requires>
    <display_length compare="ge">360</display_length>
  </requires>
  <supports>
    <control>keyboard</control>
    <control>pointing</control>
    <control>touch</control>
  </supports>
  <categories>
    <category>System</category>
  </categories>
  <keywords>
    <keyword>Fingerprint</keyword>
  </keywords>
  <content_rating type="oars-1.1" />
</component>

